{
  "name": "Meeting Cost",
  "description": "The hidden costs of meetings",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/calendar/*"],
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "content.js"
      ]
    }
  ]
}
